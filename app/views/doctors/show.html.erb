<h1> <%= @doctor.name%>'s Appointments</h1>
  <h2> Appointments: </h2>
  <ul>
    <% @doctor.appointments.each do |appointment| %>
      <% @doctor.patients.each do |patient|%>
        <li><%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M") %> with <%= link_to patient.name, patient_path(patient) %></li><br>
      <% end %>
    <% end %>
  </ul>
