<h1> <%= @patient.name%>'s Appointments</h1>
  <h2> Appointments: </h2>
  <ul>
    <% @patient.appointments.each do |appointment| %>
      <% @patient.doctors.each do |doctor|%>
        <li><%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M") %> with <%= link_to doctor.name, doctor_path(doctor) %></li><br>
      <% end %>
    <% end %>
  </ul>
